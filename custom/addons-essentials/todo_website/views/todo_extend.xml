<?xml version="1.0" encoding="utf-8"?>
<odoo>

    <template id="hello_extended"
              name="Extend Hello World"
              inherit_id="todo_website.hello">
        <xpath expr="//h1" position="replace">
            <h1>
                Hello <t t-esc="name or 'Someone'"/>!
            </h1>
        </xpath>
    </template>
</odoo>

